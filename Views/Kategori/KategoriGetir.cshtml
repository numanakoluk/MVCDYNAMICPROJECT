@model MVCDYNAMICPROJECT.Models.Entity.TBLKATEGORILER

@{
    ViewBag.Title = "KategoriGetir";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using (Html.BeginForm("Guncelle", "Kategori", FormMethod.Post))
{

    <div class="form-group">
        @Html.LabelFor(m => m.KATEGORI)
        @Html.TextBoxFor(m => m.KATEGORI, new { @class = "form-control" })
        <br />
        @Html.LabelFor(m=>m.KATEGORIAD)
        @Html.TextBoxFor(m=>m.KATEGORIAD, new {@class = "form-control"})
    </div>
    <div>
        <button class="btn btn-warning">Kategori Güncelle</button>
    </div>
}

